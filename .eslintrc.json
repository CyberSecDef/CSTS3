{
    "extends": "airbnb-base",
    "rules" : {
        "global-require" : "off",
        "prefer-destructuring" : "off",
        "no-console" : "off",
        "no-underscore-dangle" : "off"
    },
    "globals" : {
        "ko" : true,
        "nw" : true,
        "csts" : true,
        "$" : true,
        "window" : true,
        "reloadWatcher" : true,
        "document" : true,
        "Blob" : true,
        "navigator" : true,
        "URL" : true ,
        "DOMParser" : true ,
        "jsPDF" : true
    }
}